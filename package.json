{
  "name": "venus",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "node": "ts-node server/www/index.ts",
    "nodemon": "nodemon server/www/index.ts",
    "nodemon-dev": "cross-env dev=true nodemon server/www/index.ts",
    "jest": " cross-env test=true jest server/__test__/*.test.ts",
    "jestall": "cross-env test=true jest --watchAll server/__test__/*.test.ts",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm",
    "entities": "npm run typeorm -- entity:create --config server/config/tconfig.ts --connection --d server/typeorm/entity --name",
    "create": "npm run typeorm -- migration:create --config server/config/tconfig.ts --connection --d server/typeorm/migrations --name 'migrations'",
    "generate": "npm run typeorm -- migration:generate --config server/config/tconfig.ts --connection --d server/typeorm/migrations --name 'migrations'",
    "rm-media": "rm server/schema/assets/media/*",
    "reset": "cross-env test=true ts-node server/utils/reset.ts",
    "webpack-prod": "cross-env prod=true webpack --config config/webpack.prod.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.8",
    "@angular/common": "~11.2.8",
    "@angular/compiler": "~11.2.8",
    "@angular/core": "~11.2.8",
    "@angular/forms": "~11.2.8",
    "@angular/platform-browser": "~11.2.8",
    "@angular/platform-browser-dynamic": "~11.2.8",
    "@angular/router": "~11.2.8",
    "apollo-cache-control": "^0.12.0",
    "apollo-server-express": "^2.23.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "cowsay": "^1.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "faker": "^5.5.3",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-upload": "^10.0.0",
    "joiful": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "nodemailer": "^6.5.0",
    "nodemon": "^2.0.7",
    "reflect-metadata": "^0.1.13",
